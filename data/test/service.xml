<service>
    <service specs="mysql" name="mysql" version="5.7">
        <env>
            name mysql
        </env>
    </service>
    <service name="redis">
        <env>
            name redis
        </env>
    </service>
    <service name="rabbitmq">
        <env>
            name rabbitmq
        </env>
    </service>
    <service name="mongodb">
        <env>
            name mongodb
        </env>
    </service>
    <service name="haproxy">
        <env>
            name haproxy
        </env>
        <dependency>
            <service name="emb"></service>
        </dependency>
    </service>
    <service name="emb">
        <env>
            name emb
        </env>
        <dependency>
            <service name="rabbitmq"></service>
            <service name="mysql"></service>
            <service name="mongodb"></service>
        </dependency>
    </service>
    <service name="blogapi">
        <env>
            name blogapi
        </env>
        <dependency>
            <service name="mysql"></service>
            <service name="redis"></service>
            <service name="haproxy"></service>
            <service name="rabbitmq"></service>
        </dependency>
    </service>
    <service name="nginx" version="1.12.3">
        <env>
            name nginx
        </env>
        <dependency>
            <service name="blogapi"></service>
        </dependency>
    </service>
</service>